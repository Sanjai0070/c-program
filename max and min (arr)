#include<stdio.h>
int main()
{
  int a[5]={1,2,3,4,5},i,j,max,min;
  int size=sizeof(a)/sizeof(a[0]);
  max=a[0];
  min=a[0];
  for(i=0;i<size;i++)
  {
    if(a[i]>max)
    {
      max=a[i];
    }
    if(a[i]<min)
    {
      min=a[i];
    }
  }
  printf("maximum number is %d and minimum number is %d",max,min);
  return 0;
  }
